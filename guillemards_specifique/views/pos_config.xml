<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="pos_config_view_form_guillemards" model="ir.ui.view">
        <field name="name">pos.config.form.view.guillemards</field>
        <field name="model">pos.config</field>
        <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='title']" position="after">
                <div id="branch" class="col-12 col-lg-6 o_setting_box">
                    <div class="o_setting_right_pane">
                        <label for="branch_id"/>
                        <div><field name="branch_id"/></div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>

</odoo>
